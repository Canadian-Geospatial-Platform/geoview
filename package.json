{
    "name": "geo-view",
    "version": "0.1.0",
    "description": "Lightweight viewer based on Leaflet and React for the Canadian Geospatial Platform",
    "private": true,
    "main": "app.tsx",
    "scripts": {
        "serve": "webpack serve --progress --color --config webpack.dev.js",
        "build": "webpack --progress --color --config webpack.prod.js",
        "deploy": "gh-pages -d dist",
        "test": "jest",
        "update_test": "jest --updateSnapshot",
        "lint": "eslint 'src/**/*.{ts,tsx,js}' 'tests/**/*.{ts,tsx,js}'",
        "fix": "eslint 'src/**/*.{ts,tsx,js}' 'tests/**/*.{ts,tsx,js}' --fix"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Canadian-Geospatial-Platform/GeoView"
    },
    "jest": {
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/react",
            "<rootDir>/node_modules/react-dom",
            "<rootDir>/node_modules/react-addons-test-utils"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "ts",
            "tsx"
        ]
    },
