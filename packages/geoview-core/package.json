    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "lodash-webpack-plugin": "^0.11.6",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.5",
    "terser-webpack-plugin": "^5.3.1",
    "glob": "^7.2.0",
    "typedoc": "^0.23.14",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-bundle-size-analyzer": "^3.1.0",
